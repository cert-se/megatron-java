<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="se.sitic.megatron.entity">
	<class
		name="Contact"
		table="contact"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>
		<property
      name="OrganizationId"
      column="org_id"
      type="integer"
      not-null="true"
      length="8"
    />
		<property
			name="FirstName"
			column="first_name"
			type="string"
			not-null="false"       
			length="128"
		/>
		<property
      name="LastName"
      column="last_name"
      type="string"
      not-null="false"       
      length="128"
    />
    <property
      name="Role"
      column="role"
      type="string"
      not-null="false"       
      length="128"
    />
    <property
      name="PhoneNumber"
      column="phone_number"
      type="string"
      not-null="false"       
      length="255"
    />
    <property
      name="EmailAddress"
      column="email_address"
      type="string"
      not-null="true"       
      length="255"
    />   
    <property
      name="EmailType"
      column="email_type"
      type="string"
      not-null="true"       
      length="4"
    />           
		<property
			name="Enabled"
			column="enabled"
			type="boolean"
			not-null="true"
		/>
		<property
			name="Comment"
			column="comment"
			type="string"
			not-null="false"
		/>
		<property
			name="Created"
			column="created"
			type="java.lang.Long"
			not-null="true"
			length="10"
		/>
		<property 
			name="LastModified"
			column="last_modified"
			type="java.lang.Long"
			not-null="true"
			length="10"
		/>
		<property
			name="ModifiedBy"
			column="modified_by"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
      name="AutoUpdateEmail"
      column="auto_update_email"
      type="boolean"
      not-null="true"
    /> 
    <property
      name="ExternalReference"
      column="external_reference"
      type="string"
      not-null="false"       
      length="255"
    />     
	</class>	
</hibernate-mapping>